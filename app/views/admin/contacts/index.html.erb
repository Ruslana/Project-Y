<%= link_to "create new contact", new_admin_contact_path %><br />
<br />
<% for contact in @contacts %>
	<%= contact.some_contact %><br />	
	<%= link_to "show", admin_contact_path(contact) %>
	<%= link_to "edit", edit_admin_contact_path(contact) %>
	<%= link_to "destroy", [:admin, contact], 
			:confirm => 'Are you sure?', 
			:method => :delete
			%>
	<br /><br />
<% end %>

<%= will_paginate @contacts %>