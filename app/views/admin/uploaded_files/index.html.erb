<%= link_to "Upload new file", new_admin_uploaded_file_path %><br /><br />

<% for upload in @uploads %>
	<%= link_to upload.music_file_name, upload.music.url %><br />
	
	<%= link_to "show", admin_uploaded_file_path(upload) %>
	<%= link_to "edit", edit_admin_uploaded_file_path(upload) %>
	<%= link_to "delete file", [:admin, upload], 
			:confirm => 'Are you sure?', 
			:method => :delete
			%>
			<br /><br />
			
<% end %>

<%= will_paginate @uploads %>