<%= link_to "new uploads", new_admin_upload_path %><br /><br />
<% for upload in @uploads %>
	<%= link_to upload.name, upload.music.url %><br />
	<%= link_to "show", admin_upload_path(upload) %>
	<%= link_to "edit", edit_admin_upload_path(upload) %>
	<%= link_to "delete file", [:admin, upload], 
			:confirm => 'Are you sure?', 
			:method => :delete
			%>
			<br /><br />
<% end %>
<br />
<%= will_paginate @uploads %>