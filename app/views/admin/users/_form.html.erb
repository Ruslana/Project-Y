<% form_for([:admin, @user]) do |f| %>
 
<p>
	<%= f.label :name %>
	<%= f.text_field :name %>
</p>
<p>
	<%= f.label :password %>
	<%= f.password_field :password %>
</p>
<p>
	<%= f.label :password_confirmation %>
	<%= f.password_field :password_confirmation %>
</p>
<p>
	<%= f.submit "Create" %>
</p>
 
<% end %>